{
  "name": "@koopjs/cli",
  "version": "0.3.3",
  "description": "CLI tool to build Koop projects",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:cli && npm run test:template",
    "test:template": "cross-env SUPPRESS_NO_CONFIG_WARNING=true mocha \"src/template/components/**/test/**/*.test.js\"",
    "test:cli": "mocha \"test/**/*.test.js\"",
    "upgrade": "node scripts/ncu.js",
    "precommit": "lint-staged && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/koop-cli.git"
  },
  "keywords": [
    "koop",
    "cli"
  ],
  "author": {
    "name": "Haoliang Yu",
    "email": "hyu@esri.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/koopjs/koop-cli/issues"
  },
  "homepage": "https://github.com/koopjs/koop-cli#readme",
  "dependencies": {
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "klaw-sync": "^6.0.0",
    "koop": "^3.11.0",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.0",
    "recast": "^0.18.1",
    "yargs": "^13.2.4"
  },
  "bin": {
    "koop": "src/cli.js"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "config": "^3.1.0",
    "cross-env": "^5.2.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "npm-check-updates": "^3.1.9",
    "proxyquire": "^2.1.0",
    "standard": "^12.0.1"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  }
}
