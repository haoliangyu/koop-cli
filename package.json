{
  "name": "@koopjs/cli",
  "version": "1.0.0-rc3",
  "description": "CLI tool to build Koop projects",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:cli && npm run test:template",
    "test:template": "cross-env SUPPRESS_NO_CONFIG_WARNING=true mocha \"src/template/components/**/test/**/*.test.js\"",
    "test:cli": "cross-env SUPPRESS_NO_CONFIG_WARNING=true mocha \"test/**/*.test.js\"",
    "ncu": "node scripts/ncu.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/koop-cli.git"
  },
  "keywords": [
    "koop",
    "cli"
  ],
  "author": {
    "name": "Haoliang Yu",
    "email": "hyu@esri.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/koopjs/koop-cli/issues"
  },
  "homepage": "https://github.com/koopjs/koop-cli#readme",
  "dependencies": {
    "dargs": "^7.0.0",
    "easy-table": "^1.1.1",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "klaw-sync": "^6.0.0",
    "koop": "^4.1.0",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "recast": "^0.20.4",
    "yargs": "^16.2.0",
    "yargs-parser": "^20.2.4"
  },
  "bin": {
    "koop": "src/cli/index.js"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "config": "^3.3.3",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "npm-check-updates": "^10.2.5",
    "proxyquire": "^2.1.3",
    "standard": "^16.0.3",
    "supertest": "^6.0.1"
  },
  "lint-staged": {
    "*.js": "standard --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  }
}
